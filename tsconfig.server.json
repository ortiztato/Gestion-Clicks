{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist",
    "target": "es2017",
    "isolatedModules": false,
    "noEmit": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  },
  "include": ["server.ts",],
  "exclude": ["node_modules"]
}
